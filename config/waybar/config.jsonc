{
  "layer": "top",
  "position": "top",
  "modules-left": [
    "hyprland/workspaces",
    "memory",
    "cpu",
    "custom/cpu_temp",
    "temperature",
    "custom/powerDraw",
    "custom/cava_mviz",
    "custom/playerctl"
  ],
  "modules-center": [
    "clock",
    "custom/weather",
    "idle_inhibitor",
    "custom/swaync"
  ],
  "modules-right": [
    "tray",
    "backlight",
    "pulseaudio",
    "custom/mic",
    "battery",
  ],
  "reload_style_on_change": true,
  "custom/logo": {
    "format": "ÔåÉ",
    "tooltip": false
  },
  "custom/cpu_temp": {
    "format": " Ôãâ {}¬∞C",
    "tooltip-format": "CPU Temp: {}¬∞C",
    "interval": 5,
    "exec": "awk '{print int($1/1000)}' /sys/class/hwmon/hwmon4/temp1_input"
  },
  "custom/gpu_temp": {
    "format": " Ôãâ {}¬∞C",
    "tooltip-format": "GPU Temp: {}¬∞C",
    "interval": 5,
    "exec": "awk '{print int($1/1000)}' /sys/class/hwmon/hwmon3/temp1_input"
  },
  "hyprland/workspaces": {
    "format": "{name}{icon}",
    "format-icons": {
      "urgent": "ÔÅ™  ",
      "active": "ÔÜí",
      "default": ""

    }
  },
  "idle_inhibitor": {
    "format": "<span font='12'>{icon} </span>",
    "format-icons": {
      "activated": "Û∞àà",
      "deactivated": "Û∞àâ"
    }
  },
  "custom/weather": {
    "format": "{}",
    "return-type": "json",
    "exec": "~/.config/waybar/scripts/weather.sh",
    "interval": 10,
    "on-click": "firefox https://wttr.in"
  },
  "custom/clipboard": {
    "format": "Óµª",
    "on-click": " cliphist list | rofi -dmenu -p 'Presse-Papier' -theme catppuccin-default | cliphist decode | wl-copy",
    "interval": 86400
  },
  "clock": {
    "format": "{:%d %B, %H:%M}",
    "interval": 1,
    "tooltip-format": "\n<big><span color='#F28FAD'>{:%Y %B}</span></big>\n<tt><small><span color='#F5C2E7'>{calendar}</span></small></tt>",
    "calendar-weeks-pos": "right",
    "today-format": "<span color='#F28FAD'><b><u>{}</u></b></span>", // Mocha for today's date
    "format-calendar": "<span color='#F5C2E7'><b>{}</b></span>", // Macchiato for calendar dates
    "format-calendar-weeks": "<span color='#F5C2E7'><b>W{:%V}</b></span>", // Macchiato for weeks
    "format-calendar-weekdays": "<span color='#F5C2E7'><b>{}</b></span>" // Macchiato for weekdays
  },
  "bluetooth": {
    "format-on": "Ôäì",
    "format-off": "",
    "format-disabled": "Û∞Ç≤",
    "format-connected": "Û∞Ç¥ ",
    "format-connected-battery": "{device_battery_percentage}% Û∞Ç¥ ",
    "tooltip-format": "{controller_alias}\t{controller_address}\n\n{num_connections} connected",
    "tooltip-format-connected": "{controller_alias}\t{controller_address}\n\n{num_connections} connected\n\n{device_enumerate}",
    "tooltip-format-enumerate-connected": "{device_alias}\t{device_address}",
    "tooltip-format-enumerate-connected-battery": "{device_alias}\t{device_address}\t{device_battery_percentage}%",
    "on-click": "blueman-manager"
  },
  "network": {
    "format-wifi": "Ôá´ ",
    "format-ethernet": "Óºâ ",
    "format-disconnected": "Ó¨Å",
    "tooltip-format": "{ipaddr}",
    "tooltip-format-wifi": "{essid} ({signalStrength}%) Ôá´ | {ipaddr}",
    "tooltip-format-ethernet": "{ifname} üñß | {ipaddr}",
    "on-click": "pkill  nm-connection-editor|| true && nm-connection-editor"
  },
  "battery": {
    "interval": 1,
    "states": {
      "good": 95,
      "warning": 30,
      "critical": 20
    },
    "format": "{capacity}%{icon} ",
    "format-charging": "{capacity}% Û∞ÇÑ Ó¨≠",
    "format-plugged": "{capacity}% Û∞ÇÑ Ó¨≠",
    "format-alt": "{time} {icon}",
    "format-full": "",
    "format-icons": [
      "Û∞Åª",
      "Û∞Åº",
      "Û∞Åæ",
      "Û∞ÇÄ",
      "Û∞ÇÇ",
      "Û∞Åπ"
    ]
  },
  "backlight": {
    "device": "intel_backlight",
    "format": "{icon}",
    "format-icons": [
      "Óèï",
      "Óèñ",
      "Óèó",
      "Óèò",
      "Óèô",
      "Óèö",
      "Óèõ",
      "Óèú",
      "Óèù",
      "Óèû",
      "Óèü"
    ],
    "on-scroll-up": "brightnessctl set +5%",
    "on-scroll-down": "brightnessctl set 5%-",
    "smooth-scrolling-threshold": 1,
    "tooltip": true,
    "tooltip-format": "Brightness: {percent}%"
  },
  "disk": {
    "interval": 30,
    "format": "ÔáÄ  {percentage_used}%",
    "path": "/"
  },
  "custom/colorpicker": {
    "format": "{}",
    "return-type": "json",
    "interval": "once",
    "exec": "~/.config/waybar/scripts/colorpicker.sh -j",
    "on-click": "sleep 1 && ~/.config/waybar/scripts/colorpicker.sh",
    "signal": 1
  },
  "cpu": {
    "interval": 1,
    "format": "Ôãõ {usage}%",
    "min-length": 6,
    "max-length": 6,
    "on-click": "btop"
  },
  "memory": {
    "format": " Óâ¶ {percentage}%",
    "on-click": "btop"
  },
  "hyprland/window": {
    "format": "( {class} )",
    "rewrite": {
      "(.*) - Mozilla Firefox": "üåé $1",
      "(.*) - zsh": "> [$1]"
    }
  },
  "temperature": {
    "format": "Ôãâ {temperatureC}¬∞C",
    "format-critical": "Ôãá {temperatureC}¬∞C",
    "interval": 1,
    "critical-threshold": 80,
    "on-click": "foot btop"
  },
  "pulseaudio": {
    "format": "{volume}% {icon}",
    "format-muted": "<span font='12'>Ó¨§</span>",
    "format-icons": {
      "headphones": "ÔÄ•",
      "bluetooth": "Û∞•∞",
      "handsfree": "ÔÄ•",
      "headset": "Û∞•∞",
      "phone": "ÔÇï",
      "portable": "ÔÇï",
      "car": "ÔÜπ",
      "default": [
        "üï®",
        "üï©",
        "üï™"
      ]
    },
    "justify": "center",
    "on-click": "amixer sset Master toggle",
    "on-click-right": "pavucontrol",
    "tooltip-format": "{icon}  {volume}%"
  },
  "jack": {
    "format": "{} Û±éî",
    "format-xrun": "{xruns} xruns",
    "format-disconnected": "DSP off",
    "realtime": true
  },
  "tray": {
    "icon-size": 14,
    "spacing": 10
  },
  "upower": {
    "show-icon": false,
    "hide-if-empty": true,
    "tooltip": true,
    "tooltip-spacing": 20
  },
  "custom/powerDraw": {
    "format": "{}",
    "interval": 1,
    "exec": "~/.config/waybar/scripts/powerdraw.sh",
    "return-type": "json"
  },
  "custom/cava_mviz": {
    "exec": "$HOME/.config/hypr/scripts/WaybarCava.sh",
    "format": "{}"
  },
  "custom/swaync": {
    "tooltip": true,
    "tooltip-format": "Left Click: Launch Notification Center\nRight Click: Do not Disturb",
    "format": "{} {icon} ",
    "format-icons": {
      "notification": "ÔÇ¢<span foreground='red'><sup>ÔëÑ</sup></span>",
      "none": "ÔÇ¢",
      "dnd-notification": "Ôá∑<span foreground='red'><sup>ÔëÑ</sup></span>",
      "dnd-none": "Ôá∑",
      "inhibited-notification": "ÔÇ¢<span foreground='red'><sup>ÔëÑ</sup></span>",
      "inhibited-none": "ÔÇ¢",
      "dnd-inhibited-notification": "Ôá∑<span foreground='red'><sup>ÔëÑ</sup></span>",
      "dnd-inhibited-none": "Ôá∑"
    },
    "return-type": "json",
    "exec-if": "which swaync-client",
    "exec": "swaync-client -swb",
    "on-click": "sleep 0.1 && swaync-client -t -sw",
    "on-click-right": "swaync-client -d -sw",
    "escape": true
  },
  "custom/playerctl": {
    "format": "<span>{}</span> ",
    "tooltip": true,
    "tooltip-format": "Left Click: Play-Pause\nRight Click: Next song\n Middle Click: Previous Song",
    "return-type": "json",
    "max-length": 25,
    "exec": "playerctl -a metadata --format '{\"text\": \"{{artist}}  {{markup_escape(title)}}\", \"tooltip\": \"{{playerName}} : {{markup_escape(title)}}\", \"alt\": \"{{status}}\", \"class\": \"{{status}}\"}' -F",
    "on-click-middle": "playerctl previous",
    "on-click": "playerctl play-pause",
    "on-click-right": "playerctl next",
    "smooth-scrolling-threshold": 1
  },
  "custom/mic": {
    "exec": "~/.config/waybar/scripts/mic-status.sh",
    "interval": 1,
    "format": "{}",
    "on-click": "~/.config/hypr/scripts/toggle-mic.sh",
    "tooltip": false
}

}
